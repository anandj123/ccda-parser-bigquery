{
    "taskGroups": [
        {
            "taskSpec": {
                "runnables": [
                    {
                        "container": {
                            "image_uri": "$IMAGE_LOCATION"
                        },
                        "environment": {
                            "variables": {
                                "GCS_LOCATION": "$GCS_LOCATION",
                                "BQ_LOCATION": "$BQ_LOCATION"
                            }
                        }
                    }
                ],
                "computeResource": {
                    "cpuMilli": 2000,
                    "memoryMib": 2000
                }
            },
            "taskCount": 1,
            "parallelism": 1
        }
    ],
    "allocationPolicy": {
        "instances": [
            {
                "policy": {
                    "machineType": "e2-medium"
                }
            }
        ]
    },
    "labels": {
        "department": "google",
        "env": "testing"
    },
    "logsPolicy": {
        "destination": "CLOUD_LOGGING"
    }
}